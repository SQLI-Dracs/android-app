<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bind="http://schemas.android.com/apk/res-auto">
    <data >

        <variable
            name="data"
            type="com.sqli.mvvmapp.mvvm.photo.model.entity.Photo" />

        <variable
            name="imageUtil"
            type="com.sqli.mvvmapp.common.utils.ImageUtils"/>
    </data>

    <com.sqli.mvvmapp.common.customview.SquareImageView
        bind:img_url="@{data.thumbnailUrl}"
        bind:image_utils="@{imageUtil}"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

</layout>